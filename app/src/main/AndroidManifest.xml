<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ctadmin.busmate">

    <uses-feature
        android:name="android.hardware.fingerprint"
        android:required="true" />

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ctadmin.activities.activities.activities.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.ctadmin.activities.activities.activities.activities.Base"/>
        <activity android:name="com.example.ctadmin.activities.activities.activities.activities.Login" />
        <activity android:name="com.example.ctadmin.activities.activities.activities.activities.Sign" />
        <activity android:name="com.example.ctadmin.activities.activities.activities.activities.Booking"/>

        <activity android:name="com.example.ctadmin.activities.activities.activities.activities.Final">

        </activity>
    </application>

</manifest>